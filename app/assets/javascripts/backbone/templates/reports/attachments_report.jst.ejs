<%
	function drawChart(attachmentData) {

	  var data = google.visualization.arrayToDataTable(attachmentData);

	  var options = {
	    title: 'Attachments',
	    hAxis: {title: 'Attachment Type', titleTextStyle: {color: 'black'}},
	    vAxis: {title: 'Number of Attachments', titleTextStyle: {color: 'black'}}
	  };

	  var chart = new google.visualization.ColumnChart(document.getElementById('chart_div'));

	  chart.draw(data, options);

	}

  setTimeout(function() {
    google.load('visualization', '1.0',
                {'callback': function() {drawChart(data.attachmentData); },
                'packages':['corechart']}
                )
  }, 1000);
%>

<div class="page-header">
  <h1 class="h1">Reports <small>attachments</small></h1>
</div>
<div id="chart_div" style="width: 900px; height: 500px;"></div>