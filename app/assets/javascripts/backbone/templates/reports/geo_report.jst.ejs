<%
  function drawGeoChart(divID) {
    var options = {
      region: 'US',
      displayMode: 'markers'
    };

    var chart = new google.visualization.GeoChart($("#" + divID)[0]);
    var dataTable = google.visualization.arrayToDataTable(obj.cityStats);

    chart.draw(dataTable, options);
  }

  google.load('visualization', '1.0', {
    callback: function() {
      drawGeoChart("geo_chart_div");
    },
    packages: ['corechart']
  })
%>

<div class="page-header">
  <h1 class="h1">Reports <small>geo</small></h1>
</div>

<div id="geo_chart_div" style="width: 900px; height: 500px;"></div>  
