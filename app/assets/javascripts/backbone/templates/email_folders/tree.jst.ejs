<ul class="folder-list m-b-md">
  <li><a id="INBOX" href="INBOX">
    <i class="fa fa-inbox "></i> Inbox
    <span class="label label-warning pull-right inbox_count_badge"><%= systemBadges.inbox %></span>
  </a></li>
  <li><a id="SENT" href="SENT">
    <i class="fa fa-envelope-o"></i> Sent Mail
  </a></li>
  <li><a id="DRAFT" href="DRAFT">
    <i class="fa fa-file-text-o"></i> Drafts <span class="badge"><%= systemBadges.draft %></span>
  </a></li>
  <li><a id="TRASH" href="TRASH">
    <i class="fa fa-trash-o"></i> Trash
  </a></li>
</ul>

<% var template = JST["backbone/templates/email_folders/tree_nodes"]; %>
<%= template({nodeName: nodeName, node: node}) %>
